    {
      "name": "Debug (CMake Build)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/ticker",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing",
